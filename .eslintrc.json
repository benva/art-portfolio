{
  "env": {
    "browser": true
  },
  "extends": "airbnb",
  "globals": {
    "document": true,
    "localStorage": true,
    "window": true
  },
  "parser": "babel-eslint",
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/jsx-one-expression-per-line": [0],
    "operator-linebreak": [0]
  }
}
